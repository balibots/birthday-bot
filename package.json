{
  "name": "telegram-bot",
  "version": "1.0.0",
  "description": "Telegram Bot that greets people, powered by Cyclic.",
  "main": "dist/bot.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/bot.js",
    "dev": "env-cmd ts-node-dev --transpile-only src/bot.ts",
    "types:watch": "tsc --watch --noEmit",
    "register": "env-cmd ts-node scripts/registerWebhook.ts",
    "test": "env-cmd jest",
    "test:watch": "env-cmd jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eludadev/telegram-bot.git"
  },
  "keywords": [
    "chat",
    "bot",
    "unicode",
    "social-media",
    "bots",
    "telegram",
    "serverless",
    "telegram-bot",
    "chatbot",
    "text-effects"
  ],
  "author": "ruiramos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eludadev/telegram-bot/issues"
  },
  "homepage": "https://github.com/eludadev/telegram-bot#readme",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/luxon": "^3.3.0",
    "@types/node": "^18.11.7",
    "@types/node-fetch": "^2.6.2",
    "@types/yargs": "^17.0.24",
    "env-cmd": "^10.1.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4",
    "yargs": "^17.7.2"
  },
  "dependencies": {
    "@cyclic.sh/dynamodb": "^0.0.34",
    "express": "^4.18.2",
    "grammy": "^1.11.2",
    "jest": "^29.6.0",
    "luxon": "^3.3.0"
  }
}
